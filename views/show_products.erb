<table>
  <tr><td> Category</td><td> Location </td><td> Name </td><td> Cost </td><td> Quantity</td><td>Total Value</tr>
  <% Product.all.each do |x| %>
  <tr><td> <%= Category.find(x.category_id).type%> </td><td> <%= Location.find(x.location_id).name%> </td><td> <%= x.name %> </td><td> <%= x.cost %></td><td><%= x.quantity %></td><td><%= x.cost * x.quantity %></td></tr>
  <% end %>
</table>
