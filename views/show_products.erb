<table>
  <tr><td>ID</td><td> Category ID </td><td> Location ID </td><td> Name </td><td> Cost </td><td> Quantity</td><td>Total Value</tr>
  <% Product.all.each do |x| %>
  <tr><td><%= x.id%> </td><td> <%= x.category_id%> </td><td> <%= x.location_id%> </td><td> <%= x.name %> </td><td> <%= x.cost %></td><td><%= x.quantity %></td><td><%= x.cost * x.quantity %></td></tr>
  <% end %>
</table>
